    "function coin_toss(n::Int64, i::Int64 = 1)\n",
    "    a = Vector{Int8}(undef, n)\n",
    "    for i in 1:n\n",
    "        a[i] = Int8(rand(0:1))\n",
    "    end\n",
    "    return a\n",
    "end"
   ]
