   "source": [
    "function pi_calc(n)\n",
    "    a0 = 2^0.5\n",
    "    b0 = 0\n",
    "    p0 = 2 + 2^0.5\n",
    "    for i in 1:n\n",
    "        a1 = (a0 + a0^-0.5)/2.0\n",
    "        b1 = ((1 + b0)*(a0^0.5))/(a0 + b0)\n",
    "        p1 = ((1 + a1)*p0*b1)/(1 + b1)\n",
    "        a0 = a1\n",
    "        b0 = b1\n",
    "        p0 = p1\n",
    "    end\n",
    "end\n",
    "@time pi_calc(10^7)"
   ]
